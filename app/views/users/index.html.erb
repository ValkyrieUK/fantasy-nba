<div id="outline" style="background-color:#1874CD; color:white;border:2px solid;border-radius:25px;box-shadow: 0px 2px 3px rgba(0, 0, 0, 0.4);border-color:black;">
<h1><%= @user.first_name %> </h1>

<% for user in @users %>
  <tr>
    <td>><%= image_tag avatar_url(user) %></td>
    <td><%= user.email %></td>
    <td><%= link_to "Show", user %></td>
    <td><%= link_to "Edit", edit_user_path(user) %></td>
    <td><%= link_to "Destroy", user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</div>